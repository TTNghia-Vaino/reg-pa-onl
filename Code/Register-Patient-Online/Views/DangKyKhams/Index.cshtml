@model IEnumerable<Register_Patient_Online.Models.DangKyKham>

@{
    ViewData["Title"] = "Danh Sách Đăng Ký Khám";
}

<h1 class="text-center">DANH SÁCH ĐĂNG KÝ KHÁM</h1>

<table class="table">
    <thead>
        <tr>
            <th>Ngày đăng kí</th>
            <th>Ngày đến khám</th>
            <th>Trạng Thái</th>
            <th>Mã Bệnh Nhân</th>
            <th>Mã Khoa</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.NgayDangKi)</td>
                <td>@Html.DisplayFor(modelItem => item.NgayDenKham)</td>
                <td>@Html.DisplayFor(modelItem => item.TrangThai)</td>
                <td>@Html.DisplayFor(modelItem => item.MaBnNavigation.MaBn)</td>
                <td>@Html.DisplayFor(modelItem => item.MaKhoaNavigation.MaKhoa)</td>
                <td>
                    
                </td>
            </tr>
        }
    </tbody>
</table>

<div class="text-center mt-3">
    <form asp-action="Create" method="get" style="display:inline;">
        <button type="submit" class="btn btn-primary">Đăng Ký Khám</button>
    </form>
</div>



